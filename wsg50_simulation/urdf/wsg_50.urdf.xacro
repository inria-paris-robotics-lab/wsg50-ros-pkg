<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="wsg_50">

<!-- finger_length: Length of finger from gripper base. (was 0.023) -->
<!-- finger_tip:    Offset of finger tip along fingers' motion direction.
                    Use e.g. for single contact point tip on finger. (default 0) -->
<xacro:macro name="wsg_50_xacro" params="name parent finger_length finger_tip *origin">

    <joint name="${name}_mounting_plate_joint" type="fixed">
        <xacro:insert_block name="origin"/>
        <parent link="${parent}"/>
        <child link="${name}_mounting_plate"/>
    </joint>


    <link name="${name}_mounting_plate">
        <inertial>
            <mass value="0.25" />
            <origin xyz="0 0 -0.1" />
        	  <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="1.0"  iyz="0.0"  izz="1.0" />
        </inertial>
    
        <visual>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                 <mesh filename="file://$(find wsg50_simulation)/meshes/FL_ISO50_WSG50.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>

    
        <collision>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                 <mesh filename="file://$(find wsg50_simulation)/meshes/FL_ISO50_WSG50.stl" scale="0.001 0.001 0.001"/>
            </geometry>
         </collision>
    </link>
    
    <!-- <gazebo reference="${name}_mounting_plate">
        <material>Gazebo/Grey</material>
        <turnGravityOff>true</turnGravityOff>
    </gazebo> -->
    
    
    
    <joint name="${name}_anterior_gripper_joint" type="fixed">
        <parent link="${name}_mounting_plate"/>
        <child link="${name}_base_link"/>
        <origin xyz="0 0 0" rpy="0 0 ${pi}" />
    </joint>

    <!-- BASE LINK -->
    <link name="${name}_base_link">
        <inertial>
            <mass value="0.25" />
            <origin xyz="0 0 0" />
            <!--inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="100.0" iyz="0.0" 
                izz="1.0" /-->
            <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="1.0"  iyz="0.0"  izz="1.0" />
        </inertial>

        <visual>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/WSG50_110.stl" scale="1 1 1"/>
            <!--box size="0.146 0.05 0.0735"/-->
            </geometry>
            <!-- <material name="grey">-->
                <material name="Robotiq-LightGrey">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
            <!--box size="0.146 0.05 0.0735"/-->
                <mesh filename="file://$(find wsg50_simulation)/meshes/WSG50_110.stl" scale="1 1 1"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="${name}_base_link">
        <material>Gazebo/Grey</material>
        <turnGravityOff>true</turnGravityOff>
    </gazebo>

    <joint name="${name}_center_joint" type="fixed">
        <origin xyz="0 0 ${finger_length}" rpy="0 0 0" />      <!--origin xyz="-0.0067 0 0.049" rpy="0 0 0" /-->
        <parent link="${name}_base_link"/>
        <child link="${name}_center" />
    </joint>


    <link name="${name}_center">
    </link>

    <!-- GRIPPER LEFT -->
    <joint name="${name}_base_joint_gripper_left" type="prismatic">
        <limit lower="-0.055" upper="-0.003" effort="1.0" velocity="1.0"/>
        <origin xyz="0 0 0" rpy="0 0 0" />      <!--origin xyz="-0.0067 0 0.049" rpy="0 0 0" /-->
        <parent link="${name}_base_link"/>
        <child link="${name}_gripper_left" />
        <!-- <dynamics friction="100" damping="100" /> -->
        <axis xyz="1 0 0"/>
        <limit effort="100" velocity="100"/>
    </joint>

    <transmission name="${name}_base_trans_left" type="pr2_mechanism_model/SimpleTransmission">
        <actuator name="${name}_base_motor_left" />
        <joint name="${name}_base_joint_gripper_left" />
        <mechanicalReduction>1</mechanicalReduction>
        <motorTorqueConstant>1</motorTorqueConstant>
    </transmission>

    <link name="${name}_gripper_left">
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
        </inertial>

        <visual>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001"/>
            <!--box size="0.0175 0.03 0.024"/-->
            </geometry>
            <material name="black">
            <color rgba="0 0 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
            <!--box size="0.0175 0.03 0.024"/-->
                <mesh filename="file://$(find wsg50_simulation)/meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </link>

    <gazebo reference="${name}_gripper_left">
        <material>Gazebo/Blue</material>
        <turnGravityOff>true</turnGravityOff>
    </gazebo>


    <!-- LEFT FINGER -->
    <joint name="${name}_guide_joint_finger_left" type="fixed">
        <origin xyz="${finger_tip} 0 ${finger_length}" rpy="0 0 0" />
        <parent link="${name}_gripper_left"/>
        <child link="${name}_finger_left" />
        <!-- <dynamics friction="100" damping="100" /> -->
        <axis xyz="1 0 0"/>
        <limit effort="100" velocity="100"/>
    </joint>

    <link name="${name}_finger_left">
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
        </inertial>

        <visual>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/WSG-FMF.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="black">
            <color rgba="0 0 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/WSG-FMF.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </link>

    <gazebo reference="${name}_finger_left">
        <material>Gazebo/Blue</material>
        <turnGravityOff>true</turnGravityOff>
    </gazebo>


    <!-- GRIPPER RIGHT -->

    <joint name="${name}_base_joint_gripper_right" type="prismatic">
        <limit lower="0.003" upper="0.055" effort="1.0" velocity="1.0"/>
        <origin xyz="0 0 0" rpy="0 0 3.14159" />
        <parent link="${name}_base_link"/>
        <child link="${name}_gripper_right" />
        <axis xyz="-1 0 0"/>
        <!-- <dynamics friction="100" damping="100" /> -->
        <limit effort="100" velocity="100"/>
    </joint>

    <transmission name="${name}_base_trans_right" type="pr2_mechanism_model/SimpleTransmission">
        <actuator name="${name}_base_motor_right" />
        <joint name="${name}_base_joint_gripper_right" />
        <mechanicalReduction>1</mechanicalReduction>
        <motorTorqueConstant>1</motorTorqueConstant>
    </transmission>
        
        <link name="${name}_gripper_right">
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
        </inertial>

        <visual>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001"/>
            <!--box size="0.0175 0.03 0.024"/-->
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
            <!--box size="0.0175 0.03 0.024"/-->
                <mesh filename="file://$(find wsg50_simulation)/meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </link>

    <gazebo reference="gripper_right">
        <material>Gazebo/White</material>
        <turnGravityOff>true</turnGravityOff>
    </gazebo>

    <!-- RIGHT FINGER -->

    <joint name="${name}_guide_joint_finger_right" type="fixed">
        <origin xyz="${finger_tip} 0 ${finger_length}" rpy="0 0 0" />
        <parent link="${name}_gripper_right"/>
        <child link="${name}_finger_right" />
        <!-- <dynamics friction="100" damping="100" /> -->
        <axis xyz="1 0 0"/>
        <limit effort="100" velocity="100"/>
    </joint>

    <link name="${name}_finger_right">
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
        </inertial>

        <visual>
            <origin xyz="0.0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/WSG-FMF.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="black">
            <color rgba="0 0 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find wsg50_simulation)/meshes/WSG-FMF.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </link>

    <gazebo reference="${name}_finger_right">
        <material>Gazebo/White</material>
        <turnGravityOff>true</turnGravityOff>
    </gazebo>

    <ros2_control name="${name}" type="actuator">
        <hardware>
            <plugin>gz_ros2_control/GazeboSimSystem</plugin>
        </hardware>


        <joint name="${name}_base_joint_gripper_right">
            <command_interface name="position"/>
            <command_interface name="velocity"/>
            <state_interface name="velocity"/>
            <state_interface name="position">
            <!-- initial position for the mock system and simulation to avoid fingers spanning into other parts-->
                <param name="initial_value">0.025</param>
            </state_interface>
        </joint>
        <joint name="${name}_base_joint_gripper_left">
            <command_interface name="velocity"/>
            <command_interface name="position"/>
            <state_interface name="velocity"/>
            <state_interface name="position">
            <!-- initial position for the mock system and simulation to avoid fingers spanning into other parts -->
                <param name="initial_value">-0.025</param>
            </state_interface>
        </joint>
    </ros2_control>
  
  <!-- <gazebo>
      <plugin filename="gz_ros2_control-system" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
      <parameters>$(find wsg50_simulation)/controllers/wsg50_driver.yaml</parameters>
      </plugin>
  </gazebo> -->

</xacro:macro>

</robot>


